<%= form_for @sentiment do |f| %>
	
	<%= f.label :text %>
	<%= f.text_field :text %>
	<br>
	<%= f.label :behavior %>
	<%= f.text_field :behavior %>
	<br>
	<%= f.label :rating %>
	<%= f.text_field :rating %>
	<br>
	<br>
	<%= f.submit 'Submit'%>

<% end %>

<h4 id="link">
	<%= link_to 'Homepage', root_path %> | 
	<%= link_to 'Complete List', sentiments_path %>
</h4>